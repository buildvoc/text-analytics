  dct:modified "2019-12-14T08:48:47.468Z"^^xsd:dateTime;
  skos:broader :c_13911a49;
  skos:definition "A mechanical or intumescent device within a duct or ventilation opening that operates automatically and is designed to resist the spread of fire."@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:prefLabel "fire damper"@en .

:c_9e8d26ab a skos:Concept;
