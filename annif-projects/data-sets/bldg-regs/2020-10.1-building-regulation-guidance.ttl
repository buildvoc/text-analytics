  dct:created "2019-12-14T08:41:43.182Z"^^xsd:dateTime;
  skos:broader :c_13911a49;
  skos:definition "The ability of a component or a building to satisfy, for a stated period of time, some or all of the appropriate criteria given in the relevant standard."@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:prefLabel "fire resisting (Fire resistance)"@en .

:c_7a2d826e a skos:Concept;
  dc:source :c_ca00c020;
