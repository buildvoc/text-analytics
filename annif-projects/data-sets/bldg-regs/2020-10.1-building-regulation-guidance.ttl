  dct:created "2019-12-14T08:38:57.474Z"^^xsd:dateTime;
  skos:broader :c_13911a49;
  skos:definition "A protected enclosure that contains a firefighting stair, firefighting lobbies and, if provided, a firefighting lift together with its machine room."@en;
  skos:inScheme :conceptScheme_751925ed;
  skos:prefLabel "firefighting shaft"@en .

:c_9a0f5e20 a skos:Concept;
  dc:source :c_ca00c020;
  dct:created "2019-12-14T08:40:17.510Z"^^xsd:dateTime;
  skos:broader :c_13911a49;
